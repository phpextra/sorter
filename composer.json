{
    "name": "phpextra/sorter",
    "description": "Sort arrays and objects by multiple fields with unicode characters",
    "keywords": ["sort", "sorter", "array", "objects", "collections", "psr-0"],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/phpextra/sorter",
    "support": {
        "issues": "https://github.com/phpextra/sorter/issues",
        "source": "https://github.com/phpextra/sorter"
    },
    "authors": [
        {
            "name": "Jacek Kobus",
            "email": "kobus.jacek@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "PHPExtra\\Sorter": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "ext-intl": ">=1.0.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.8",
        "phpunit/phpunit": "~3.7",
        "sami/sami": "~1.2",
        "phpextra/qa-tools": "~1.0"
    }
}
